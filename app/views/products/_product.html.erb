<h4>Product Name</h4>
<%= product.name %>

<h4>Description</h4>
<% if product.description.present? %>
<%= product.description %>
<% else %>
<br />
<% end %>

<h4>Product Type</h4>
<% if product.type.present? %>
<%= product.type %>
<% else %>
<br />
<% end %>

<h4>Released Date</h4>
<% if product.released_date.present? %>
<%= product.released_date %>
<% else %>
<br />
<% end %>

<h4>Tag</h4>
<% @product.tags.each do |tag| %>
	<%= link_to tag, tag_path(tag
	) %> /
<% end %>